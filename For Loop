package forloop;
import java.util.*;
public class ForLoop {
    public static void main(String[] args) {
        int choice;
        System.out.println("Enter the number : ");
        Scanner s = new Scanner(System.in);
        choice = s.nextInt();
        for(int i = 0; i <= choice; i++)
            System.out.println(i);
    }
}
