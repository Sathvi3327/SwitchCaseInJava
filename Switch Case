package loops;

import java.util.Scanner;

public class Loops {

    public static void main(String[] args) {
        int choice;
        System.out.println("Pick one : \t1. Hey\t2. Hello\t3. Hi\t");
        Scanner s = new Scanner(System.in);
        choice = s. nextInt();
        switch(choice)
        {
            case 1: System.out.println("You choosed Hey");
                    break;
            case 2: System.out.println("You choosed Hello");
                    break;
            case 3: System.out.println("You choosed Hi");
                    break;
        }
    }
    
}
